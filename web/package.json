{
  "name": "fishki-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.11.1",
    "react-scripts": "5.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-syntax-highlighter": "^15.5.13"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=true react-scripts build",
    "build:dev": "GENERATE_SOURCEMAP=true NODE_ENV=development react-scripts build",
    "test": "react-scripts test --watchAll=false --coverage --passWithNoTests",
    "test:watch": "react-scripts test --coverage --watchAll",
    "test:coverage": "react-scripts test --watchAll=false --coverage --passWithNoTests",
    "release": "npm run test && GENERATE_SOURCEMAP=false NODE_ENV=production react-scripts build",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!src/**/*.d.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!react-markdown|react-syntax-highlighter|mdast-util-*|micromark|decode-named-character-reference|character-entities|property-information|hast-util-*|space-separated-tokens|comma-separated-tokens|bail|unified|unist-util-*|vfile|markdown-table|./lib/.*)"
    ],
    "moduleNameMapper": {
      "^lodash-es$": "lodash"
    }
  },
  "proxy": "http://localhost:8080",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
